{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": ["warn", { "args": "none" }],
        "prefer-const": ["warn"],
        "no-empty-pattern": "off",
        "no-case-declarations": "off"
    }
}
